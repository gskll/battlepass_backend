datasource sqlite {
  url      = env("DATABASE_URL")
  provider = "sqlite"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model BattlePass {
  id          String   @id @default(cuid())
  name        String   @default("")
  description String   @default("")
  status      String?  @default("DRAFT")
  start_date  DateTime
  end_date    DateTime
  experience  Int
}